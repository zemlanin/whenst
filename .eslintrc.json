{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "env": {
    "browser": true,
    "es2020": true,
    "node": false
  },
  "globals": {
    "URLPattern": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": 0,
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "ignoreRestSiblings": true
      }
    ],
    "require-atomic-updates": 0
  },
  "overrides": [
    {
      "files": "icons/*.svg.jsx",
      "rules": {
        "react/display-name": 0
      }
    }
  ]
}
