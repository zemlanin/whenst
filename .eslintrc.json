{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "es2020": true,
    "node": false
  },
  "globals": {
    "URLPattern": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "ignoreRestSiblings": true
      }
    ],
    "require-atomic-updates": 0
  }
}
