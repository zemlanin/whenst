{
  "name": "whenst",
  "version": "1.0.0",
  "description": "",
  "targets": {
    "default": {
      "context": "browser",
      "source": [
        "index.html",
        "settings.html",
        "about.html",
        "link.html"
      ],
      "distDir": "./dist",
      "includeNodeModules": true
    }
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build",
    "start": "wrangler pages dev dist --kv KV",
    "lint": "npm run eslint && npm run prettier -- -l",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write",
    "prettier": "prettier \"./*.{html,css,js,jsx}\" \"./functions/**/*.js\"",
    "eslint": "eslint \"./*.{js,jsx}\" \"./functions/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://zemlanin@github.com/zemlanin/whenst.git"
  },
  "author": "Anton Verinov <anton@verinov.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zemlanin/whenst/issues"
  },
  "homepage": "https://github.com/zemlanin/whenst#readme",
  "devDependencies": {
    "@cfworker/json-schema": "^1.12.5",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@js-temporal/polyfill": "^0.4.4",
    "@parcel/packager-raw-url": "^2.9.3",
    "@parcel/service-worker": "^2.9.3",
    "@parcel/transformer-inline-string": "^2.9.3",
    "@parcel/transformer-webmanifest": "^2.9.3",
    "@preact/signals": "^1.1.5",
    "cookie": "^0.5.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "parcel": "^2.9.3",
    "preact": "^10.16.0",
    "prettier": "^3.0.0",
    "sortablejs": "^1.15.0",
    "urlpattern-polyfill": "^9.0.0",
    "wrangler": "^3.2.0"
  }
}
